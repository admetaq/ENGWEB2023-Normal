extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Planta #{p._id}
    .w3-container
      ul.w3-ul 
        li Id: #{p._id}
        li Freguesia: #{p.Freguesia}
        li Espécie: #{p.Espécie}
        li Estado: #{p.Estado}
        li Nº Intervenções: #{p['Número de intervenções']}
        li Nº de Registo: #{p['Número de Registo']}
        li Código de rua: #{p['Código de rua']}
        li Rua: #{p.Rua}
        li Local: #{p.Local}
        li Nome Científico: #{p['Nome Científico']}
        li Origem: #{p.Origem}
        li Data de Plantação: #{p['Data de Plantação']}
        li Caldeira: #{p.Caldeira}
        li Tutor: #{p.Tutor}
        li Implantação: #{p.Implantação}
        li Gestor: #{p.Gestor}
        li Data de atualização: #{p['Data de atualização']}
                
    footer.w3-container.w3-teal
      h5 Generated Plantas::EngWeb2023 in #{d}